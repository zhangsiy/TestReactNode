{
  "name": "ReactNodeTest",
  "version": "1.0.0",
  "description": "A test project to play with react",
  "main": "server.js",
  "author": "jeff zhang",
  "scripts": {
    "watch": "watchify -d public/scripts/index.js -o public/scripts/bundle.js -v",
    "build": "browserify public/scripts/index.js -o public/scripts/bundle.js",
    "start": "npm run watch & nodemon server.js"
  },
  "devDependencies": {
    "redux-devtools": "~3.2.0"
  },
  "dependencies": {
    "jquery": "~1.11.3",
    "express": "~4.11.1",
    "mongoose": "~3.8.0",
    "body-parser": "~1.4.2",
    "method-override": "~2.0.2",
    "react": "~0.14.8",
    "react-dom": "~0.14.8",
    "babel-loader": "~6.2.0", 
    "babel-core": "~6.7.5",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-stage-0": "~6.5.0",
    "browserify": "~6.0.3",
    "nodemon": "~1.8.1",
    "uglify-js": "~2.4.15",
    "watchify": "~3.6.1",
    "babelify": "~7.2.0",
    "redux": "~3.1.2",
    "react-redux": "~4.1.2",
    "material-ui": "~0.14.4",
    "react-bootstrap": "~0.28.5",
    "express-jwt": "~3.3.0",
    "react-router": "~2.0.0"
  },
  "browserify": {
    "transform": [["babelify", 
    { 
        "presets": ["es2015", "react", "stage-0"]
    }]]
  }
